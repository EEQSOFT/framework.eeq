{
    "name": "eeqsoft/framework.eeq",
    "type": "project",
    "license": "proprietary",
    "description": "A minimal framework project recommended to create applications",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/panther": "^2.0",
        "dbrekelmans/bdi": "^1.0"
    }
}
